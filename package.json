{
  "name": "@studiokloek/ts-core-lib",
  "version": "2.0.0-48",
  "description": "Core TypeScript lib for our projects.",
  "author": "Martijn Swart <opensource@studiokloek.nl> (https://studiokloek.nl)",
  "license": "MIT",
  "exports": {
    ".": "./lib/index.js",
    "./package.json": "./package.json"
  },
  "types": "lib/index.d.ts",
  "repository": "github:studiokloek/ts-core-lib",
  "bugs": {
    "url": "https://github.com/studiokloek/ts-core-lib/issues"
  },
  "scripts": {
    "clean": "rimraf ./lib",
    "release": "npm run build && np",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run clean && npm run build:script",
    "build:types": "tsc --emitDeclarationOnly",
    "build:script": "tsc"
  },
  "np": {
    "yarn": false,
    "tests": false
  },
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "files": [
    "lib/*"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@capacitor/app": "^1.0.6",
    "@capacitor/core": "^3.3.2",
    "@capacitor/device": "^1.1.0",
    "@capacitor/network": "^1.0.6",
    "@capacitor/splash-screen": "^1.1.6",
    "@capacitor/status-bar": "^1.0.6",
    "@capacitor/storage": "^1.2.3",
    "@types/fontfaceobserver": "2.1.0",
    "@types/howler": "^2.2.4",
    "@types/lodash-es": "^4.17.5",
    "@types/superagent": "^4.1.13",
    "bowser": "^2.11.0",
    "fontfaceobserver": "^2.1.0",
    "gsap": "3.7.1",
    "howler": "^2.2.3",
    "lodash-decorators-esm": "^6.1.1",
    "lodash-es": "^4.17.21",
    "pixi-spine": "^3.0.13",
    "pixi.js": "~6.2.0",
    "pubsub-js": "^1.9.4",
    "random-js": "^2.1.0",
    "ress": "^4.0.0",
    "slugify": "^1.6.2",
    "superagent": "^6.1.0",
    "ts-events": "^3.4.0",
    "ts-mixer": "^6.0.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@capacitor/cli": "^3.3.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-unicorn": "^36.0.0",
    "np": "^7.5.0",
    "npm": "8.1.3",
    "prettier": "^2.4.1",
    "prettier-package-json": "^2.6.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.2"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 iOS versions",
    "last 2 ChromeAndroid versions"
  ]
}
