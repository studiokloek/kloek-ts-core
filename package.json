{
  "name": "@studiokloek/ts-core-lib",
  "version": "2.0.7",
  "description": "Core TypeScript lib for our projects.",
  "author": "Martijn Swart <opensource@studiokloek.nl> (https://studiokloek.nl)",
  "license": "MIT",
  "exports": {
    ".": "./lib/index.js",
    "./package.json": "./package.json"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "github:studiokloek/ts-core-lib",
  "bugs": {
    "url": "https://github.com/studiokloek/ts-core-lib/issues"
  },
  "scripts": {
    "clean": "rimraf ./lib",
    "release": "npm run build && np",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run clean && npm run build:script",
    "build:types": "tsc --emitDeclarationOnly",
    "build:script": "tsc"
  },
  "np": {
    "yarn": false,
    "tests": false
  },
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "files": [
    "lib/*"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@capacitor/app": "^1.1.1",
    "@capacitor/core": "^3.6.0",
    "@capacitor/device": "^1.1.2",
    "@capacitor/network": "^1.0.7",
    "@capacitor/splash-screen": "^1.2.2",
    "@capacitor/status-bar": "^1.0.8",
    "@capacitor/storage": "^1.2.5",
    "@types/fontfaceobserver": "2.1.0",
    "@types/howler": "2.2.7",
    "@types/lodash-es": "^4.17.6",
    "@types/superagent": "^4.1.15",
    "bowser": "^2.11.0",
    "fontfaceobserver": "^2.3.0",
    "gsap": "3.10.4",
    "howler": "^2.2.3",
    "lodash-decorators-esm": "^6.1.1",
    "lodash-es": "^4.17.21",
    "pixi-spine": "^3.0.15",
    "pixi.js": "^6.4.2",
    "pubsub-js": "^1.9.4",
    "random-js": "^2.1.0",
    "ress": "^5.0.2",
    "slugify": "^1.6.5",
    "superagent": "^7.1.6",
    "ts-events": "^3.4.1",
    "ts-mixer": "^6.0.1",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "np": "^7.6.1",
    "npm": "8.12.2",
    "prettier": "^2.7.1",
    "prettier-package-json": "^2.6.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 iOS versions",
    "last 2 ChromeAndroid versions"
  ]
}
