{
  "name": "@studiokloek/ts-core-lib",
  "version": "1.26.3",
  "description": "Core TypeScript lib for our projects.",
  "author": "Martijn Swart <opensource@studiokloek.nl> (https://studiokloek.nl)",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "github:studiokloek/ts-core-lib",
  "bugs": {
    "url": "https://github.com/studiokloek/ts-core-lib/issues"
  },
  "scripts": {
    "clean": "rimraf ./lib",
    "release": "yarn build && np --any-branch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -- --watch",
    "build": "yarn clean && yarn build:types && yarn build:js",
    "build:watch": "babel src --watch --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "build:types": "tsc --emitDeclarationOnly",
    "build:script": "tsc",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  },
  "np": {
    "tests": false
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "files": [
    "lib/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.21.5",
    "@capacitor/core": "^2.5.0",
    "@types/fontfaceobserver": "2.1.0",
    "@types/howler": "2.2.7",
    "@types/lodash-es": "^4.17.7",
    "@types/stats.js": "^0.17.0",
    "@types/superagent": "^4.1.17",
    "bowser": "^2.11.0",
    "fontfaceobserver": "^2.3.0",
    "gsap": "^3.7.1",
    "howler": "^2.2.3",
    "lodash-es": "^4.17.21",
    "pixi-spine": "3.1.2",
    "pixi.js": "~6.5.9",
    "pubsub-js": "^1.9.4",
    "random-js": "^2.1.0",
    "slugify": "^1.6.6",
    "stats.js": "^0.17.0",
    "superagent": "^6.1.0",
    "ts-events": "^3.4.1",
    "ts-mixer": "5.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "core-js": "^3.30.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-unicorn": "^27.0.0",
    "lodash-decorators": "^6.0.1",
    "np": "^7.7.0",
    "prettier": "^2.8.8",
    "prettier-package-json": "^2.8.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 iOS versions",
    "last 2 ChromeAndroid versions"
  ]
}
