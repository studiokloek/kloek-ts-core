{
  "name": "@studiokloek/ts-core-lib",
  "version": "1.25.0",
  "description": "Core TypeScript lib for our projects.",
  "author": "Martijn Swart <opensource@studiokloek.nl> (https://studiokloek.nl)",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "github:studiokloek/ts-core-lib",
  "bugs": {
    "url": "https://github.com/studiokloek/ts-core-lib/issues"
  },
  "scripts": {
    "clean": "rimraf ./lib",
    "release": "npm run build && np --any-branch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "build:watch": "babel src --watch --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "build:types": "tsc --emitDeclarationOnly",
    "build:script": "tsc",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  },
  "np": {
    "yarn": false,
    "tests": false
  },
  "engines": {
    "node": ">=12.20.1",
    "npm": ">=6.14.10"
  },
  "files": [
    "lib/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@babel/runtime-corejs3": "^7.16.3",
    "@capacitor/core": "^2.5.0",
    "bowser": "^2.11.0",
    "fontfaceobserver": "^2.1.0",
    "gsap": "^3.7.1",
    "howler": "^2.2.3",
    "lodash-es": "^4.17.21",
    "pixi-spine": "^2.1.14",
    "pixi.js-legacy": "^5.3.11",
    "pubsub-js": "^1.9.4",
    "random-js": "^2.1.0",
    "slugify": "^1.6.3",
    "stats.js": "^0.17.0",
    "superagent": "^6.1.0",
    "ts-events": "^3.4.0",
    "ts-mixer": "5.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/fontfaceobserver": "0.0.7",
    "@types/howler": "^2.2.4",
    "@types/lodash-es": "^4.17.5",
    "@types/stats.js": "^0.17.0",
    "@types/superagent": "^4.1.13",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "core-js": "^3.19.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-unicorn": "^27.0.0",
    "lodash-decorators": "^6.0.1",
    "np": "^7.6.0",
    "npm": "^7.24.2",
    "prettier": "^2.4.1",
    "prettier-package-json": "^2.6.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.4"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "not Chrome < 60",
    "last 2 Safari versions",
    "not Safari < 10.1",
    "last 2 iOS versions",
    "not iOS < 10.3",
    "last 2 Firefox versions",
    "not Firefox < 54",
    "last 2 Edge versions",
    "not Edge < 15"
  ]
}
